{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "cst.png"
   },
   "content_scripts": [ {
      "css": [ "nStyles.css" ],
      "matches": [ "http://*/*", "https://*/*", "file://*/*" ]
   } ],
   "description": "Test your css selector by using this quick script. Does not support the \"HTML\" Element or \"*\" selector.",
   "manifest_version": 2,
   "name": "CSS Selector Tester",
   "permissions": [ "\u003Call_urls\u003E", "tabs"],
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "1.3.2"
}
