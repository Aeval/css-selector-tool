{
  "name": "CSS Selector Tester",
  "version": "1.1",
  "manifest_version": 3,
  "description": "Test your css selector by using this quick script. Does not support the \"HTML\" Element or \"*\" selector.",
  "browser_action": {
    "default_icon": "icon_css_test.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
    "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "css": ["nStyles.css"]
    }
  ],
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}

